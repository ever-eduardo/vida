var rand = load("std/rand")
var N = 1_000_000

var randRange = fun a, b {
    assert(b > a)
    ret rand.nextI(b - a) + a
}

var makeList = fun N, a, b {
    var xs = list(N)
    for i in 0, N {
        xs[i] = randRange(a, b)
    }
    ret xs
}

var checkRange = fun xs, a, b {
    for _, v in xs {
        assert(a <= v and v < b)
    }
}

var a = 1_000_000
var b = 2_000_000

checkRange(makeList(N, a, b), a, b)
